<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd 
">
<mapper namespace="dao.DevUserMapper">
<select id="getlogin" resultType="devUser">
      select * from dev_user  where devCode=#{devCode}
</select>
<select id="getStutes" resultType="dataDictionary">
      select * from data_dictionary where typeCode="APP_STATUS"
</select>
<select id="getFlatform" resultType="dataDictionary">
      select * from data_dictionary where typeCode="APP_FLATFORM"
</select>
<select id="getAll" resultType="appCategory">
      select * from app_category where parentId=0
</select>
<select id="getSecond" resultType="appCategory">
      select * from app_category  where parentId = #{id}
</select>
<select id="getSan" resultType="appCategory">
      select * from app_category  where parentId = #{id}
</select>
</mapper>